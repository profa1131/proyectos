{% extends '::base.html.twig' %}

{% block body -%}
   
   <h1 class="page-header, fi-torsos-all-female"> Detalles de Familias</h1>
   
   <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    Datos de Familias 
                </div>
                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                     <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Apellidos</th>
                <td>{{ entity.apellidos }}</td>
            </tr>
            <tr>
                <th>Telefonolocal</th>
                <td>{{ entity.telefonoLocal }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ entity.direccion }}</td>
            </tr>
            <tr>
                <th>Sector</th>
                <td>{{ entity.sector }}</td>
            </tr>
            <tr>
                <th>Nombrescomunidad</th>
                <td>{{ entity.nombresComunidad }}</td>
            </tr>
        </tbody>
                </table>
            </div>
         </div>
   </div>

    <table class="record_properties">
       
    </table>

        <ul class="record_actions">
   
            <a class="btn btn-primary" href="{{ path('familias') }}">Volver a la Lista </a> <br>
        </a>
    
   
        <a class="btn btn-success" href="{{ path('familias_edit', { 'id': entity.id }) }}">Editar Registro </a>
        
   
   {{ form(delete_form) }}
</ul>
{% endblock %}
